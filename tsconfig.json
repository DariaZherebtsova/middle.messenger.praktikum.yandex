{
    "compilerOptions": {
        "outDir": "dist",
        "target": "es2016",
        "lib": ["es2016", "dom", "es2018.promise"],
        "declaration": false,
        "module": "commonjs",
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "strictNullChecks": true,
        "esModuleInterop": true
    }
}
